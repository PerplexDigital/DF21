{
  "$schema": "./umbraco/config/appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": ""
  },
  "Umbraco": {
    "CMS": {
      "Hosting": {
        "Debug": false
      },
      "Content": {
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "Global": {
        "Id": "621295f9-0783-4d8f-9b29-e101fdf89f3a",
        "UseHttps": true
      },
      "ModelsBuilder": {
        "ModelsMode": "SourceCodeManual"
      },
      "WebRouting": {
        "DisableRedirectUrlTracking": true
      }
    }
  },
  "uSync": {
    "Settings": {
      "ExportOnSave": "Settings"
    }
  }
}
